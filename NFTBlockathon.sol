abstract contract MinterRole {
    mapping(address => bool) public isMinter;

    event MintersUpdated(address[] , bool[] status);

    modifier onlyMinter {
        require(isMinter[msg.sender], "Caller is not minter");
        _;
    }

    function _setMinters(address[] memory minters, bool[] memory isActives) internal {
        for(uint256 i = 0; i < minters.length; i++) {
            isMinter[minters[i]] = isActives[i];
        }

        emit MintersUpdated(minters, isActives);
    }

    function setMinters(address[] memory minters, bool[] memory isActives) external virtual;
}





contract StarshipCollection is VRC725Enumerable, MinterRole {
    string public baseURI;

    event BaseURIUpdated(string newBaseURI);
    
    constructor(string memory name, string memory symbol, string memory initBaseURI, address issuer) {
        isMinter[issuer] = true;
        baseURI = initBaseURI;

        __VRC725_init(name, symbol, issuer);
    }

    function setMinters(address[] memory minters, bool[] memory isActives) external override onlyOwner {
        _setMinters(minters, isActives);
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory newBaseURI) external onlyOwner {
        baseURI = newBaseURI;

        emit BaseURIUpdated(newBaseURI);
    }

    function mint(address owner) external onlyMinter {
        uint256 currentIndex = totalSupply();
        _safeMint(owner, currentIndex + 1);
    }

    function mintMulti(address owner, uint256 amount) external onlyMinter {
        for (uint i = 0; i < amount; i++) {
            uint256 currentIndex = totalSupply();
            _safeMint(owner, currentIndex + 1);
        }
    }
}